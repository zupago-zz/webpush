{
    "name": "zupago/webpush",
    "description": "Web Push Notifications driver for Laravel.",
    "homepage": "https://github.com/zupago/webpush",
    "license": "MIT",
    "authors": [
        {
            "name": "ZuPago",
            "email": "developer@zupago.pe",
            "homepage": "http://zupago.pe",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.0",
        "illuminate/notifications": "^5.3",
        "illuminate/support": "^5.1",
        "minishlink/web-push": "^2.0"
    },
    "require-dev": {
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~6.0",
        "orchestra/testbench": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\webpush\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "NotificationChannels\\webpush\\WebPushServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true,
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
